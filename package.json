{
  "name": "nextsupabase",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "db:generate": "drizzle-kit generate",
    "db:gentypes": "supabase gen types --lang=typescript --local > src/types/database.types.ts",
    "db:migrate": "drizzle-kit migrate",
    "db:nuke": "supabase db reset",
    "db:push": "drizzle-kit push",
    "db:reset": "supabase db reset && drizzle-kit migrate",
    "db:reset:seed": "supabase db reset && drizzle-kit migrate && bun run ./src/db/seed/index.ts",
    "db:seed": "bun run ./src/db/seed/index.ts",
    "db:start": "supabase start",
    "db:stop": "supabase stop --project-id nextsupabase",
    "dev": "next dev --turbopack",
    "format": "prettier . --write",
    "lint": "next lint",
    "lint:fix": "eslint --fix",
    "prepare": "husky",
    "start": "next start",
    "storybook": "storybook dev -p 6006"
  },
  "overrides": {
    "react": "npm:react@19.0.0"
  },
